{
  "name": "krux-installer",
  "version": "0.0.1-alpha-1",
  "author": [
    "qlrd <106913782+qlrd@users.noreply.github.com>"
  ],
  "description": "An imager installer for Krux firmwares",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint:client": "vue-cli-service lint",
    "lint:public": "eslint public/",
    "lint:server": "eslint src/",
    "icon:svg": "aasvg < ./build/krux.txt > ./build/krux.svg",
    "icon:png": "svgexport ./build/krux.svg ./build/krux.png",
    "icon:resize": "resize-img --width 256 --height 256 ./build/krux.png > ./build/icon.png",
    "icon:ico": "png-to-ico ./build/icon.png > ./build/icon.ico",
    "icon:icns": "mk-icns ./build/icon.png ./build",
    "icon": "yarn run icon:svg && yarn run icon:png && yarn run icon:resize && yarn run icon:ico && yarn run icon:icns",
    "lint": "yarn run lint:server && yarn run lint:public && yarn run lint:client",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "yarn run lint && DEBUG=krux:* vue-cli-service electron:serve",
    "electron:serve:win": "yarn run lint && SET DEBUG=krux:* && vue-cli-service electron:serve",
    "postinstall:platform": "node ./bin/platform-dependencies.js",
    "postinstall:electron": "electron-builder install-app-deps",
    "postinstall": "yarn run postinstall:platform && yarn run postinstall:electron",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "@nathanielks/electron-sudo": "^4.0.15",
    "ansi_up": "^5.1.0",
    "axios": "^0.27.2",
    "core-js": "^3.26.1",
    "debug": "^4.3.4",
    "electron-store": "^8.1.0",
    "hasbin": "^1.2.3",
    "roboto-fontface": "^0.10.0",
    "vue": "^3.2.45",
    "vuetify": "^3.0.1",
    "webfontloader": "^1.6.28",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "aasvg": "^0.3.3",
    "electron": "21.3.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.7.0",
    "make-icns": "^1.1.4",
    "png-to-ico": "^2.1.8",
    "replace": "^1.2.2",
    "resize-img-cli": "^1.0.0",
    "svgexport": "^0.4.2",
    "vue-cli-plugin-electron-builder": "^3.0.0-alpha.4",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "webpack-plugin-vuetify": "^2.0.0"
  }
}
